<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.upasana.home.popper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash_Activity"
            android:theme="@style/SplashTheme">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".Level_2">
        </activity>
        <activity android:name=".Level_3">
        </activity>
        <activity android:name=".Level_4">
        </activity>
        <activity android:name=".Level_5">
        </activity>
        <activity android:name=".Level_6">
        </activity>
        <activity android:name=".Level_screen">
        </activity>
        <activity android:name=".StartUp">
        </activity>
    </application>

</manifest>